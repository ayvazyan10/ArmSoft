{
    "name": "ayvazyan10/armsoft",
    "description": "This Laravel package allows you to interact with the ArmSoft API easily",
    "license": "MIT",
    "authors": [
        {
            "name": "Razmik Ayvazyan",
            "email": "ayvazyan403@gmail.com",
            "homepage": "https://ayvazyan.pro"
        }
    ],
    "homepage": "https://github.com/ayvazyan10/armsoft",
    "keywords": [
        "Laravel",
        "ArmSoft",
        "REST",
        "API"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ayvazyan10\\ArmSoft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ayvazyan10\\ArmSoft\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ayvazyan10\\ArmSoft\\ArmSoftServiceProvider"
            ],
            "aliases": {
                "ArmSoft": "Ayvazyan10\\ArmSoft\\Facades\\ArmSoft"
            }
        }
    }
}
